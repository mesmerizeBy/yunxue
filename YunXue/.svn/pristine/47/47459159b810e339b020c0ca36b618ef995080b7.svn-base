			.search-title{margin-top: 12px; padding-left: 15px; height: 36px; line-height: 36px; background: #f4f4f4; border: 1px solid 
				#eae9e9; border-left: 0; border-right: 0;}
			.search-title a:hover{color:#f90;}
			.search-title a{color: black; font-size: 14px;}
			.crumbs-nav-item{float: left; margin-right: 8px;}
			.crumbs-arrow {font-family: simsun;font-style: normal;}
			.menu-drop { display: inline-block; height: 24px; cursor: pointer; }
			.menu-drop:hover { text-decoration: none; }
			.menu-drop .menu-drop-arrow { display: inline-block; width: 20px; height: 20px; vertical-align: top; background: 
				url(../images/icon/menu-icon.png) no-repeat 4px 7px; -webkit-transition: background-position .15s ease-in-out; 
				-moz-transition: background-position .15s ease-in-out; transition: background-position .15s ease-in-out; }
			.crumbs-nav { height: 24px; line-height: 24px; }
			.crumbs-nav-item .menu-drop{float: left; margin-right: 5px;}
			.crumbs-nav-item .menu-drop a.big{color: #666; font-size: 18px; font-weight: bold;}
			.menu-drop .trigger { display: inline-block; margin-top: 6px; height: 22px; padding: 0 4px 0 8px; background: #fff; border: 
				1px solid #ddd; line-height: 22px; vertical-align: top; }
			.menu-drop .trigger .curr { display: inline-block; vertical-align: top; }
			.menu-drop-main { display: none; width: 350px; padding: 10px 0 10px 8px; position: absolute; left: 0; top: 23px; border: 1px 
				solid #ddd; background-color: #fff; }
			.menu-drop-list li { float: left;  width: 65px; height: 24px; overflow: hidden; line-height: 24px; margin-right: 5px; 
				text-overflow: ellipsis; white-space: nowrap; }
			.menu-drop:hover .trigger{border-color: #1ec800;}
			.selector-set .ss-item { position: relative;  display: inline-block; height: 22px; line-height: 22px; border: 1px solid 
				#ddd; font-size: 14px; vertical-align: top; margin: 6px 5px 5px 0; padding: 0 28px 0 7px; cursor: pointer; background: 
					#fff;}
			.selector-set .ss-item b { font-weight: 400; }
			.selector-set .ss-item em { color: #666; font-style: normal;}
			.selector-set .ss-item i { display: block; position: absolute; width: 25px; height: 22px; right: 0; top: 0; background: 
				url(../images/icon/menu-icon.png) no-repeat 7px -122px; }
			.selector-set .ss-item:hover i { background-position: 7px -139px; }
			.selector-set .ss-item:hover { border-color: #1ec800; text-decoration: none; }
			.menu-drop-main a:hover{color: #1ec800;}
			.z-menu-drop-open{position:relative;z-index:5}
			.z-menu-drop-open .trigger{border-bottom-color:#fff;position:relative;z-index:6}
			.z-menu-drop-open .menu-drop-arrow{background-position:4px -27px}
			.z-menu-drop-open .menu-drop-main{display:block}
			.z-menu-drop-open:hover .trigger{border-bottom-color:#fff}
			.z-menu-drop-open:hover .menu-drop-main{border-color:#1ec800}
			.z-menu-drop-open:hover .menu-drop-arrow{background-position:4px -44px}
