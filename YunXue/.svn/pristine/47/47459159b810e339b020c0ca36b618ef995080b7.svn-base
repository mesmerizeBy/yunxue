@charset "utf-8";
/* CSS Document */
*{
	outline: none;
}
.container {
    width: 1300px;
    margin: 0 auto;
}
.query_img{
	width:1300px;
	height: auto;
}
.clearboth:after {
    display: block;
    content: "clear";
    height: 0;
    clear: both;
    overflow: hidden;
    visibility: hidden;
}

li a{
	text-decoration: none;
	color:rgba(86,83,83,1.00);
}
.teacher_list_style1 li img {
    width: 70px;
    height: 70px;
    border-radius: 50%;
    float: left;
    margin-right: 40px;
}
img {
    border: none;
}
.teacher_list_style1 li p.title {
    font-size: 18px;
    line-height: 18px;
}
.teacher_list_style1 .title span {
    font-size: 14px;
    color: #000000;
    margin-left:25px;
    font-family:"微软雅黑";
    font-weight:bold;
}
.teacher_list_style1 li p.description {
    line-height: 18px;
    color: #999;
    font-size: 12px;
    margin-top: 8px;
    height:70px;
    overflow: hidden;
}
.teacher_list_style1 li p.description span{
	font-size: 13px;
    color: #000000;
}
.teacher-style-title {
    margin-top: 15px;
    line-height: 14px;
}
.page {
    height: 36px;
    margin-right: auto;
    margin-left: auto;
    padding: 20px 0;
    text-align: center;
}
.page a.first, .page a:hover {
    background: #4d88d9;
    border: #4d88d9 solid 1px;
    color: #fff;
}
.page a{
    display: inline-block;
    width: 50px;
    height: 34px;
    border: #ddd solid 1px;
    border-radius: 3px;
    text-align: center;
    line-height: 34px;
    margin: 0 2px;
    transition: all 0.2s ease 0s;
    font-size: 12px;
    background: #f6f6f6;
    color: #333;
}
.page a.lesson-right-btn, .page a.lesson-left-btn {
    width: 90px;
}
