.service{
	width: 70px;
    height: 214px;
    position: fixed;
    z-index: 12;
    right: 10px;
    bottom: 120px;}

.s1{
	cursor: pointer;
    width: 70px;
    height: 54px;
    margin: 1px 0;
    padding-top: 16px;
    background-color: #FC0;
    -webkit-transition: .3s;
    transition: .3s;
    border-radius: 2px;}

.s1:hover{
	opacity:0.8;}
	
.s2{
	cursor: pointer;
    width: 70px;
    height: 40px;
    margin: 1px 0;
    padding-top: 16px;
    background-color:rgb(247,247,247);
    -webkit-transition: .3s;
    transition: .3s;
    border-radius: 2px;}

.s2:hover{
	opacity:0.8;}	
	
